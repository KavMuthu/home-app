using System;
using System.Collections.Generic;
using System.Linq;
using System.Web;

namespace SurveyPIT.Models
{
    /// <summary>
    /// Individual will be household of one
    /// </summary>
    public class PITHousehold
    {
        public int PITHouseholdId { get; set; }
        /// <summary>
        /// Model: Observation or Interview
        /// </summary>
        public string Model { get; set; }
        /// <summary>
        /// Age 18 or older
        /// </summary>
        public int Adult { get; set; }
        public int Children { get; set; }
        /// <summary>
        /// This is for observation
        /// </summary>
        public int NotSureAdultOrChildren { get; set; }
        public double Latitude { get; set; }
        public double Longitude { get; set; }
        public DateTime CreatedDate { get; set; }
        public int TypeOfLocation { get; set; }
        /// <summary>
        /// Login id of volunteer or out reach workers
        /// </summary>
        public int UserId { get; set; }
    }
}