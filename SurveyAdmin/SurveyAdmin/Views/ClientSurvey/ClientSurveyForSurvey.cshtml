@model System.Linq.IQueryable<SurveyAdmin.Models.SurveyInfo.ClientSurvey>

@{
}

<table class="table">

    @foreach (var cs in Model)
    {
        <tr>
            <td>
                @cs.ClientSurveyId
            </td>
            <td>
                @{ var firstName = cs.Responses.FirstOrDefault(q => q.Question.QuestionText == "First Name:"); }
                @if (firstName != null)
                {
                    @firstName.Value
                }
            </td>
            <td>
                @{ var lastName = cs.Responses.FirstOrDefault(q => q.Question.QuestionText == "Last Name:"); }
                @if (lastName != null)
                {
                    @lastName.Value
                }
            </td>
            <td>
                @Html.ActionLink("View ROI", MVC.ROI.Get(cs.ClientSurveyId))
                
            </td>
            @*@foreach (var r in cs.Responses)
            {
                <td>@r.Value</td>
            }*@
        </tr>
    }
</table>