<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <gradient android:angle="270" android:endColor="@color/button_positive_dark" android:startColor="@color/button_positive" />
            <stroke android:width="1dp" android:color="@color/button_positive_stroke" />
            <size android:width="40dp" android:height="40dp" />
            <padding android:bottom="6dp" android:left="6dp" android:right="6dp" android:top="6dp" />
        </shape>
    </item>
    <item>
        <bitmap android:src="@drawable/plus" android:antialias="false" android:dither="true" android:filter="false" android:mipMap="false" />
    </item>
</layer-list>

